{
    "name": "renedekat/blmreader",
    "type": "library",
    "description": "BLM Reader to parse BLM files and output as CSV, Excel, Collection or Array",
    "keywords": [
        "renedekat",
        "blmreader"
    ],
    "homepage": "https://github.com/renedekat/blmreader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ren√© de Kat",
            "email": "renedekat@9lives-development.com",
            "homepage": "https://9lives-development.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "renedekat/php-verbal-expressions": "^1.0",
        "tightenco/collect": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ReneDeKat\\Blm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReneDeKat\\Blm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
